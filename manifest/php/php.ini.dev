date.timezone = "Europe/London"
session.save_path = "/var/sessions"
post_max_size = 768M
upload_max_filesize = 768M
memory_limit = 64M

opcache.enable = false



[xdebug]
zend_extension=/usr/local/lib/php/extensions/debug-non-zts-20190902/xdebug.so
xdebug.remote_enable=1
xdebug.remote_handler=dbgp
xdebug.remove_port=9000
xdebug.remote_autostart=1
xdebug.remote_connect_back=0
xdebug.idekey=docker
xdebug.remote_host=192.168.99.1

[ext]
extension=mcrypt.so

display_errors = On
error_reporting = On